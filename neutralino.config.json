{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.neutralino.sample",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/react-app/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.readDirectory",
    "debug.log"
  ],
  "globalVariables": {
    "TEST1": "Hello",
    "TEST2": [
      2,
      4,
      5
    ],
    "TEST3": {
      "value1": 10,
      "value2": {}
    }
  },
  "modes": {
    "window": {
      "title": "neu-mg-sqlnpx",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/react-app/public/vite.svg",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/react-app/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "cli": {
    "binaryName": "neu-mg-sqlnpx",
    "resourcesPath": "/react-app/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0",
    "copyItems": [
      "/MDBQueryVemovel.exe",
      "MDBQueryVemovel.exe"
    ],
    "frontendLibrary": {
      "patchFile": "/react-app/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/react-app/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}